# Maintainer: Danila Fominykh <dancheg97@fmnx.su>

pkgname=registry
pkgver='0.1.0'
pkgrel=1
pkgdesc="Self-hosted registry for arch packages."
arch=('x86_64')
url="https://fmnx.su/core/pack"
license=('GPL')
depends=()
makedepends=('go')

build() {
  cd ..
  go build -o src/p .
}

package() {
  install -Dm755 p $pkgdir/usr/bin/registry
}
